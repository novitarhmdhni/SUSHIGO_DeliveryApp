<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity_user">

    <ImageView
        android:id="@+id/imgprofil"
        android:layout_width="82dp"
        android:layout_height="82dp"
        android:background="@drawable/profile"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.05"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-100dp"
        android:text="USER"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.274" />

    <Button
        android:id="@+id/btnEdit"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginTop="35dp"
        android:text="EDIT PROFILE"
        tools:layout_editor_absoluteX="163dp"
        tools:ignore="MissingConstraints"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"/>

    <Button
        android:id="@+id/btnStatus"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginTop="5dp"
        android:text="STATUS ORDER"
        tools:layout_editor_absoluteX="163dp"
        tools:ignore="MissingConstraints"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnEdit"/>

    <Button
        android:id="@+id/btnVouc"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginTop="5dp"
        android:text="VOUCHER SUSHIGO"
        tools:layout_editor_absoluteX="163dp"
        tools:ignore="MissingConstraints"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnStatus"/>

    <Button
        android:id="@+id/btnHub"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginTop="5dp"
        android:text="HUBUNGI RESTO"
        tools:layout_editor_absoluteX="163dp"
        tools:ignore="MissingConstraints"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnVouc"/>

    <Button
        android:id="@+id/btnLogout"
        android:layout_width="350dp"
        android:layout_height="70dp"
        android:layout_marginTop="50dp"
        android:textSize="16dp"
        android:textStyle="bold"
        android:text="LOG OUT"
        tools:layout_editor_absoluteX="163dp"
        tools:ignore="MissingConstraints"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnHub"/>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:layout_marginLeft="11dp"
        android:layout_marginRight="11dp"
        android:layout_marginBottom="150dp"
        android:layout_marginTop="130dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="10dp"
        app:layout_constraintTop_toBottomOf="@+id/btnLogout">

        <ImageView
            android:id="@+id/imghome"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="13dp"
            android:layout_marginTop="17dp"
            android:background="@drawable/house"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imglist"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="108dp"
            android:layout_marginLeft="13dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/clipboard1"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imgwallet"
            android:layout_width="38dp"
            android:layout_height="38dp"
            android:layout_marginStart="210dp"
            android:layout_marginLeft="13dp"
            android:layout_marginTop="17dp"
            android:background="@drawable/wallet"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imgprofile"
            android:layout_width="43dp"
            android:layout_height="43dp"
            android:layout_marginStart="305dp"
            android:layout_marginLeft="13dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/user1"
            android:textStyle="bold" />
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>